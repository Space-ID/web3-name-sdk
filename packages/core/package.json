{
  "name": "@web3-name-sdk/core",
  "version": "0.1.4",
  "description": "One stop Web3 domain name resolution",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup --format esm,cjs --dts --clean",
    "lint": "prettier --write .",
    "test": "ts-mocha -p tsconfig.json tests/**/*.spec.ts",
    "test3": "ts-mocha -p tsconfig.json tests/**/sidV3.spec.ts"
  },
  "author": "SPACE ID",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "lodash": "^4.17.21",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "tsup": "^7.1.0"
  },
  "peerDependencies": {
    "@bonfida/spl-name-service": "^1.4.0",
    "@injectivelabs/networks": "1.14.0-beta.1",
    "@injectivelabs/sdk-ts": "1.14.0-beta.1",
    "@injectivelabs/ts-types": "1.14.0-beta.1",
    "@sei-js/core": "^3.1.0",
    "@siddomains/injective-sidjs": "0.0.2-beta",
    "@siddomains/sei-sidjs": "^0.0.3",
    "@solana/web3.js": "^1.87.1",
    "lodash": "^4.17.21"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./seiName": {
      "types": "./dist/seiName.d.mts",
      "default": "./dist/seiName.mjs"
    },
    "./injName": {
      "types": "./dist/injName.d.mts",
      "default": "./dist/injName.mjs"
    },
    "./solName": {
      "types": "./dist/solName.d.mts",
      "default": "./dist/solName.mjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.mts",
      "default": "./dist/utils/index.mjs"
    }
  },
  "types": "dist/index.d.ts",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "@adraffy/ens-normalize": "^1.10.0",
    "@ensdomains/ens-validation": "^0.1.0",
    "viem": "^1.10.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/injName.ts",
      "src/seiName.ts",
      "src/solName.ts",
      "src/utils/index.ts"
    ]
  },
  "keywords": [
    "eth"
  ]
}